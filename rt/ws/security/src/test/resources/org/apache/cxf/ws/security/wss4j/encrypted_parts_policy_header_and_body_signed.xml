<?xml version="1.0" encoding="UTF-8"?>
<wsp:Policy 
    xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy" 
    xmlns:sp="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702">
  <wsp:ExactlyOne>
    <wsp:All>
      <sp:SymmetricBinding>
        <wsp:Policy>
          <sp:ProtectionToken>
            <wsp:Policy>
              <sp:X509Token sp:IncludeToken="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702/IncludeToken/Always">
                <wsp:Policy>
                  <sp:RequireEmbeddedTokenReference />
                  <sp:WssX509V3Token10 />
                </wsp:Policy>
              </sp:X509Token>
            </wsp:Policy>
          </sp:ProtectionToken>
          <sp:AlgorithmSuite>
            <wsp:Policy>
              <sp:Basic128 />
            </wsp:Policy>
          </sp:AlgorithmSuite>
          <sp:Layout>
            <wsp:Policy>
              <sp:Strict />
            </wsp:Policy>
          </sp:Layout>
          <sp:EncryptBeforeSigning/>
        </wsp:Policy>
      </sp:SymmetricBinding>
      <sp:SignedParts>
        <sp:Body/>
        <sp:Header Namespace="http://www.sdj.pl"/>
      </sp:SignedParts>
      <sp:EncryptedParts>
        <sp:Body/>
        <sp:Header Name="Header" Namespace="http://www.sdj.pl"/>
      </sp:EncryptedParts>  
      <sp:EncryptedParts>
        <sp:Body/>
        <sp:Header Namespace="http://www.sdj.pl"/>
      </sp:EncryptedParts>
    </wsp:All>
  </wsp:ExactlyOne>
</wsp:Policy>
