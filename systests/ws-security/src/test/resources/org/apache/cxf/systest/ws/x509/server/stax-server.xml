       wsdlLocation="org/apache/cxf/systest/ws/x509/DoubleItX509.wsdl">
        
       <jaxws:properties>
          <entry key="ws-security.callback-handler" 
                  value="org.apache.cxf.systest.ws.wssec10.client.KeystorePasswordCallback"/>
